    "email": "raynos2@gmail.com"
  },
  "contributors": [
    {
      "name": "Raynos"
    },
    {
      "name": "heapwolf"
    }
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/optoolco/append-fs-logger/raw/master/LICENSE"
    }
  ],
  "binDependencies": {
    "standard": "14.3.1"
  },
  "devDependencies": {
    "@pre-bundled/tape": "4.11.0",
    "collapsed-assert": "1.0.3",
    "error": "10.2.0",
    "npm-bin-deps": "1.3.2",
    "resultify": "1.1.2",
